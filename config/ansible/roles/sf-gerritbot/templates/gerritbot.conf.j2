[ircbot]
nick={{ gerritbot['botname'] }}
pass={{ gerritbot['password'] }}
server={{ gerritbot['ircserver'] }}
port={{ gerritbot['ircport'] }}
channel_config=/etc/gerritbot/channels.yaml
pid=/var/run/gerritbot/gerritbot.pid
log_config=/etc/gerritbot/logging.conf

[gerrit]
user=jenkins
key=/var/lib/gerritbot/.ssh/id_rsa
host=gerrit
port=29418
